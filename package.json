{
  "name": "chalees-min",
  "version": "0.1.0",
  "description": "Learning!",
  "private": true,
  "main": "index.js",
  "scripts": {
    "_prepare-deploy": "npm run build && fireplan security-rules.yaml",
    "dev": "fireplan security-rules.yaml && firebase deploy --only=\"database,storage\" && webpack-dev-server --hot --inline --history-api-fallback --content-base dist/ --progress --host 0.0.0.0",
    "optim-dev": "PRODUCTION=true webpack-dev-server --history-api-fallback --content-base dist/ --progress --compress",
    "build": "PRODUCTION=true webpack --progress",
    "build-size": "npm run build -s -- --json | webpack-bundle-size-analyzer",
    "deploy": "DEPLOY_TARGET=production npm run _prepare-deploy && firebase deploy --project \"chalees-min\" && ./open-site-in-browser.sh",
    "stage": "DEPLOY_TARGET=staging npm run _prepare-deploy && firebase --project \"chalees-staging\" deploy && ./open-site-in-browser.sh --staging",
    "_ci-prepare-deploy": "npm run ci-build && fireplan security-rules.yaml",
    "ci-build": "PRODUCTION=true webpack",
    "ci-deploy-staging": "DEPLOY_TARGET=staging npm run _ci-prepare-deploy && firebase --project \"chalees-staging\" --token=$FIREBASE_TOKEN_STAGING --non-interactive deploy",
    "ci-deploy-production": "DEPLOY_TARGET=production npm run _ci-prepare-deploy && firebase --project \"chalees-min\" --token=$FIREBASE_TOKEN_PRODUCTION --non-interactive deploy",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolasartman/chalees-min.git"
  },
